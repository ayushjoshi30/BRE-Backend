      {
        "name": "Loantovaluecalculator",
        "content": "/**\r\n * @param input\r\n * @param {{\r\n *  dayjs: import('dayjs')\r\n *  Big: import('big.js').BigConstructor\r\n * }} helpers\r\n */\r\nconst handler = (input, { dayjs, Big }) => {\r\n  const { Profile, DOB, Loanamountrequested } = input.customer;\r\n  const { Gridvalue, \"Insurence value\": InsurenceValue, \"Market value\": MarketValue } = input.Value;\r\n\r\n  const AssetValue = (Number(Gridvalue) + Number(InsurenceValue) + Number(MarketValue)) / 3;\r\n  const LTVRatio = (Number(Loanamountrequested) / AssetValue) * 100;\r\n  const Eligibilityamount = AssetValue * LTVRatio;\r\n  return { LTVRatio, Eligibilityamount };\r\n}\r\n\r\n\r\n",
        "id": "ffb6a8ef-9516-4fc7-8234-117566a8cc67",
        "position": {
          "x": 35,
          "y": -200
        },
        "type": "functionNode"
      },
